{
  "name": "fs-readdir-promise",
  "version": "1.0.1",
  "description": "Promises/A+ version of fs.readdir",
  "repository": "shinnn/fs-readdir-promise",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/fs-readdir-promise/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "fs",
    "files",
    "read",
    "readdir",
    "dir",
    "directory",
    "path",
    "filename",
    "promise",
    "promises",
    "then",
    "thenable"
  ],
  "dependencies": {
    "graceful-fs": "^3.0.5",
    "wrap-promise": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^0.10.2",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
}
